@model IEnumerable<MVcInterview.Models.CandidateDetailsT.CandidateDetailsT>

@{
    ViewData["Title"] = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h1>Index</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.CandidateName)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.CandidateExperience)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.CandidatePhoneNo)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.CandidateMailId)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.CandidateSkills)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.CreatedDate)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.CandidateName)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.CandidateExperience)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.CandidatePhoneNo)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.CandidateMailId)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.CandidateSkills)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.CreatedDate)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.CandidateId">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.CandidateId">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.CandidateId">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
